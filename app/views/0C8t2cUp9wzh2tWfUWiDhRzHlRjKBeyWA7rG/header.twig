        {# <!-----notification popup---------> #}
        {% for pop in popupNotif %}
        {% if pop["title"] %}
        <div class="modal fade" popIds="{{ popupNotif["notifIds"] }}" id="notificationModal_{{ loop.index }}"
          tabindex="-1" aria-labelledby="notificationModalLabel_{{ loop.index }}" aria-hidden="true"
          data-keyboard="false" data-backdrop="static">
          <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="notificationModalLabel">{{ pop["title"] }}</h5>
                <button type="button" class="close" data-dismiss="modal" data-toggle="modal"
                  data-target="#notificationModal_{{ loop.index+1 }}" aria-label="Close">
                  <span aria-hidden="true">×</span>
                </button>
              </div>
              <div class="modal-body">
                {{ pop["description"]|replace({"\n": "<br />"})  }}
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-actions" style="background:#;" data-dismiss="modal"
